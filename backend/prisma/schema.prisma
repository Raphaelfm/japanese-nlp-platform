generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model TextAnalysis {
  id         Int     @id @default(autoincrement())
  text       String
  translated String?
  sentiment  String?
  createdAt  DateTime @default(now())
}

model User {
  id       Int     @id @default(autoincrement())
  username String  @unique
  password String
}